	<queryString>
		<![CDATA[SELECT
    u.email as Email,
    e.title as Title,
    e.description as Description,
    e.location as Location,
    e.start as "Start Date",
    e.end as "End Date"
from users u INNER join events e
on e.user_id = u.id
where $X{IN,e.category_id,category_ids} and u.email like $P{email}
order by e.id;]]>
	</queryString>
